{
  "name": "@nestjsplus/cookies",
  "version": "1.1.1",
  "description": "Decorators for Managing Cookies with NestJS",
  "author": "John Biundo <johnfbiundo@gmail.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "scripts": {
    "build": "rm -rf ./dist && tsc && npm run build:index",
    "build:index": "rm -rf ./index.js ./index.d.ts && tsc -d --skipLibCheck ./index.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nestjs",
    "decorators",
    "cookies"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nestjsplus/cookies"
  },
  "bugs": "https://github.com/nestjsplus/cookies/issues",
  "dependencies": {
    "lodash": "^4.17.15",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4"
  },
  "peerDependencies": {
    "@nestjs/common": ">6.0.0",
    "@nestjs/core": ">6.0.0",
    "@nestjs/platform-express": ">6.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/common": "^7.0.6",
    "@nestjs/core": "^7.0.6",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.1",
    "@types/express": "^4.17.3",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "cookie-parser": "^1.4.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "hbs": "^4.1.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "rimraf": "^2.6.2",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
